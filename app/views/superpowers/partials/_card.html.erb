<div class="card-container">
  <div class="card-img" style="background-image: linear-gradient(to top, rgba(0, 0, 0, 1) 0%, rgba(0, 0, 0, 0) 50%), url('https://www.placecage.com/g/155/300')">
     <div class="category-container">
        <%= image_tag ("categories/fingering.png") %>
       <p> <%= superpower.superpower_category.name %>  </p>
    </div>
    <div class="card-price">
      <p> <%= superpower.price %> <span> / DAY</span> </p>
    </div>
  </div>
  <div class="card-text">
    <h3> <%= superpower.name %> </h3>
    </div>
</div>

